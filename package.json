{
  "name": "raml-documentation-viewer",
  "version": "0.1.1",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "A documentation viewer for RAML file. This is the main element to be used to display a documentation from a RAML file",
  "devDependencies": {
    "polymer-cli": "^0.16.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "raml-documentation-viewer.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint raml-documentation-viewer.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"raml-documentation-viewer:local-test\"",
    "test": "node_modules/.bin/polymer test",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/raml-documentation-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/raml-documentation-viewer/issues",
    "email": "arc@mulesoft.com"
  }
}